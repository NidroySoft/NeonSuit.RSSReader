{
  "ConnectionStrings": {
    "RssReaderDatabase": "Data Source=rssreader.db"
  },
  "AppSettings": {
    "AutoRefreshEnabled": true,
    "RefreshIntervalMinutes": 30,
    "MinimizeToTray": true,
    "StartWithWindows": false,
    "Theme": "Light"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },
  "UpdateSettings": {
    "CheckForUpdates": true,
    "UpdateFeedUrl": "https://neonsuit.com/updates.xml"
  }
}