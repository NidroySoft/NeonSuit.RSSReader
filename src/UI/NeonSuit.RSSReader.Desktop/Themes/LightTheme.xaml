<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="WindowBackgroundColor">#FFFFFF</Color>
    <Color x:Key="SecondaryBackgroundColor">#F3F3F3</Color>
    <Color x:Key="PrimaryTextColor">#2D2D2D</Color>
    <Color x:Key="SecondaryTextColor">#686868</Color>
    <Color x:Key="AccentColor">#005A9E</Color>
    <SolidColorBrush x:Key="WindowBackgroundBrush" Color="{StaticResource WindowBackgroundColor}"/>
    <SolidColorBrush x:Key="PrimaryTextBrush" Color="{StaticResource PrimaryTextColor}"/>
    <SolidColorBrush x:Key="AccentBrush" Color="{StaticResource AccentColor}"/>
</ResourceDictionary>